generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model post_travels {
  id          String   @id @default(cuid())
  title       String
  description String
  createdAt   DateTime @default(now())
  country     String
  city        String
  image       String
  user        User?     @relation(fields: [userId], references: [id])
  userId      String?
}

model User {
  id       String       @id @default(cuid())
  email    String       @unique
  name     String?
  password String?
  posts    post_travels[]
}
